<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.module.category" id="module_category_equation_coffee_contact_customization">
            <field name="name">Equation Coffee Contacts</field>
            <field name="description">Allow the user to view and manage the models and actions to be added in Odoo for the management of the Equation Coffee company.</field>
            <field name="sequence">21</field>
        </record>

        <record id="group_equation_contacts_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_equation_coffee_contact_customization"/>
            <!-- <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/> -->
        </record>

        <record id="group_equation_contacts_admin" model="res.groups">
            <field name="name">Administrator</field>
            <field name="category_id" ref="module_category_equation_coffee_contact_customization"/>
            <!-- <field name="implied_ids" eval="[(4, ref('group_equation_coffee_user'))]"/> -->
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>